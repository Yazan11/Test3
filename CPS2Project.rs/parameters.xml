<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="actuatorEffectiveness" displayName="Actuator Effectiveness" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="surfaceTemp" displayName="Surface Temperature (degC)" type="float" 
						defaultValue="30.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="initialRPM" displayName="Initial RPM (0 - 100)" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="toolVotingWeight" displayName="Voting weight of a tool (Indicate tool ID)" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="nbSensor" displayName="Number of Sensor Agents" type="int" 
						defaultValue="5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="holeDiameter" displayName="Hole Diameter (inch)" type="float" 
						defaultValue="6.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="drillingAngle" displayName="Drilling Angle (degree)" type="double" 
						defaultValue="30.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="nbActuator" displayName="Number of Uphole Actuator Agents" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="WOBIncreaseAmount" displayName="WOB Increase per Tick (klbf)" type="double" 
						defaultValue="0.05" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="downHoleRPMPercentage" displayName="DownHole RPM Percentage (0-100)" type="float" 
						defaultValue="50.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="startingRunDepth" displayName="Starting Run Depth (m)" type="double" 
						defaultValue="10.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="votingEnabled" displayName="Voting Enabled" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="weightOnBit" displayName="WOB (0 to 30 klbf)" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="depthGoal" displayName="Total Depth (m)" type="int" 
						defaultValue="550" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="coolingEnabled" displayName="Cooling Enabled" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="constantWOBincrease" displayName="WOB constant increase" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="hardFormation" displayName="Hard Formation" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="geothermalGradient" displayName="Geothermal Gradient (degC/m)" type="float" 
						defaultValue="0.03" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="numDifferentLayers" displayName="Number of Different Layers" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="votingRuleChoice" displayName="Voting Rule Choice (1: On/Off, 2: Plurality, 3: Condorcet, 4: Borda, 5: Proposed Rule)" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="heightDifferentLayers" displayName="Height of Different Layers" type="double" 
						defaultValue="0.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		</parameters>


